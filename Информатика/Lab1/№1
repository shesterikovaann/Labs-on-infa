// вариант 4
#include <iostream>
int main()
{
	setlocale(LC_ALL, "rus");
	double speed;
	std::cin >> speed;
	if (speed < 60.0) {
		std::cout << "Скорость в пределах нормы" << std::endl;
	}
	else if (speed <= 80.0) {
		std::cout << "Вы приближаетесь к превышению скорости" << std::endl;
	}
	else {
		std::cout << "Превышение скорости" << std::endl;
	}
}
